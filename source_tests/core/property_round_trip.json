            {
              "key": "",
              "value": "item2"
            },
            {
              "key": "regular",
              "value": "value"
            }
          ]
        },
        {
          "function": "round_trip",
          "expect": null
        }
      ],
      "features": [
        "empty_keys"
      ]
    },
    {
      "name": "round_trip_nested_structures",
      "input": "config =\n  host = localhost\n  port = 8080\n  db =\n    name = mydb\n    user = admin",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "config",
