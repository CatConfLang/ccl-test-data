{
  "$schema": "../../schemas/source-format.json",
  "tests": [
    {
      "name": "round_trip_basic",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "value"
            },
            {
              "key": "nested",
              "value": "\n  sub = val"
            }
          ]
        },
        {
          "function": "print",
          "expect": "key = value\nnested = \n  sub = val"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "inputs": [
        "key = value\nnested =\n  sub = val"
      ]
    },
    {
      "name": "round_trip_whitespace_normalization",
      "tests": [
        {
          "function": "round_trip",
          "expect": true
        },
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "value  \n  nested  = \n    sub  =  val"
            }
          ]
        }
      ],
      "features": [
        "whitespace"
      ],
      "behaviors": [
        "baseline_zero"
      ],
      "variants": [
        "reference_compliant"
      ],
      "conflicts": {
        "behaviors": ["baseline_first_key"]
      },
      "inputs": [
        "  key  =  value  \n  nested  = \n    sub  =  val  "
      ]
    },
    {
      "name": "round_trip_whitespace_normalization_baseline_first_key",
      "tests": [
        {
          "function": "round_trip",
          "expect": true
        },
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "value"
            },
            {
              "key": "nested",
              "value": "\n    sub  =  val"
            }
          ]
        }
      ],
      "features": [
        "whitespace"
      ],
      "behaviors": [
        "baseline_first_key"
      ],
      "conflicts": {
        "behaviors": ["baseline_zero"]
      },
      "inputs": [
        "  key  =  value  \n  nested  = \n    sub  =  val  "
      ]
    },
    {
      "name": "round_trip_empty_keys_lists",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "item1"
            },
            {
              "key": "",
              "value": "item2"
            },
            {
              "key": "regular",
              "value": "value"
            }
          ]
        },
        {
          "function": "print",
          "expect": " = item1\n = item2\nregular = value"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "= item1\n= item2\nregular = value"
      ]
    },
    {
      "name": "round_trip_nested_structures",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "config",
              "value": "\n  host = localhost\n  port = 8080\n  db =\n    name = mydb\n    user = admin"
            }
          ]
        },
        {
          "function": "print",
          "expect": "config = \n  host = localhost\n  port = 8080\n  db =\n    name = mydb\n    user = admin"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "inputs": [
        "config =\n  host = localhost\n  port = 8080\n  db =\n    name = mydb\n    user = admin"
      ]
    },
    {
      "name": "round_trip_multiline_values",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "script",
              "value": "\n  #!/bin/bash\n  echo hello\n  exit 0"
            }
          ]
        },
        {
          "function": "print",
          "expect": "script = \n  #!/bin/bash\n  echo hello\n  exit 0"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "features": [
        "multiline"
      ],
      "inputs": [
        "script =\n  #!/bin/bash\n  echo hello\n  exit 0"
      ]
    },
    {
      "name": "round_trip_mixed_content",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "name",
              "value": "Alice"
            },
            {
              "key": "",
              "value": "first item"
            },
            {
              "key": "config",
              "value": "\n  port = 3000"
            },
            {
              "key": "",
              "value": "second item"
            },
            {
              "key": "final",
              "value": "value"
            }
          ]
        },
        {
          "function": "print",
          "expect": "name = Alice\n = first item\nconfig = \n  port = 3000\n = second item\nfinal = value"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "name = Alice\n= first item\nconfig =\n  port = 3000\n= second item\nfinal = value"
      ]
    },
    {
      "name": "round_trip_complex_nesting",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "app",
              "value": "\n  = item1\n  config =\n    = nested_item\n    db =\n      host = localhost\n      = db_item\n  = item2"
            }
          ]
        },
        {
          "function": "print",
          "expect": "app = \n  = item1\n  config =\n    = nested_item\n    db =\n      host = localhost\n      = db_item\n  = item2"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "app =\n  = item1\n  config =\n    = nested_item\n    db =\n      host = localhost\n      = db_item\n  = item2"
      ]
    },
    {
      "name": "round_trip_deeply_nested",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "level1",
              "value": "\n  level2 =\n    level3 =\n      level4 =\n        deep = value\n        = deep_item"
            }
          ]
        },
        {
          "function": "print",
          "expect": "level1 = \n  level2 =\n    level3 =\n      level4 =\n        deep = value\n        = deep_item"
        },
        {
          "function": "round_trip",
          "expect": true
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "level1 =\n  level2 =\n    level3 =\n      level4 =\n        deep = value\n        = deep_item"
      ]
    },
    {
      "name": "round_trip_empty_multiline",
      "tests": [
        {
          "function": "round_trip",
          "expect": true
        },
        {
          "function": "parse",
          "expect": [
            {
              "key": "empty_section",
              "value": ""
            },
            {
              "key": "other",
              "value": "value"
            }
          ]
        },
        {
          "function": "print",
          "expect": "empty_section = \nother = value"
        }
      ],
      "features": [
        "empty_keys",
        "multiline"
      ],
      "inputs": [
        "empty_section =\n\nother = value"
      ]
    }
  ]
}
