{
  "$schema": "../../schemas/source-format.json",
  "tests": [
    {
      "name": "tabs_as_content_in_value",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "\tvalue\twith\ttabs"
            }
          ]
        },
        {
          "function": "build_hierarchy",
          "expect": {
            "key": "\tvalue\twith\ttabs"
          }
        },
        {
          "function": "get_string",
          "args": [
            "key"
          ],
          "expect": "\tvalue\twith\ttabs"
        }
      ],
      "behaviors": [
        "tabs_as_content"
      ],
      "features": [
        "whitespace"
      ],
      "inputs": [
        "key = \tvalue\twith\ttabs"
      ]
    },
