{
  "$schema": "../../schemas/source-format.json",
  "tests": [
    {
      "name": "spacing_strict_standard_format",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "value"
            }
          ]
        }
      ],
      "behaviors": [
        "strict_spacing",
        "loose_spacing"
      ],
      "features": [
        "whitespace"
      ],
      "inputs": [
        "key = value"
      ]
    },
    {
      "name": "spacing_loose_no_spaces",
      "tests": [
