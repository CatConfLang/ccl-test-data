    {
      "name": "basic_list_from_duplicates",
      "input": "servers = web1\nservers = web2\nservers = web3",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "servers",
              "value": "web1"
            },
            {
              "key": "servers",
              "value": "web2"
            },
            {
              "key": "servers",
              "value": "web3"
            }
          ]
        },
        {
          "function": "build_hierarchy",
          "expect": {
            "servers": [
              "web1",
              "web2",
              "web3"
            ]
          }
        },
        {
          "function": "get_list",
          "expect": [
            "web1",
            "web2",
            "web3"
          ],
          "args": [
            "servers"
          ]
        }
      ]
    },
