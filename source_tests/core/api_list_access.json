    {
      "name": "list_with_comments",
      "input": "servers = web1\n/= Production servers\nservers = web2\nservers = web3\n/= End of list",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "servers",
              "value": "web1"
            },
            {
              "key": "/",
              "value": "Production servers"
            },
            {
              "key": "servers",
              "value": "web2"
            },
            {
              "key": "servers",
              "value": "web3"
            },
            {
              "key": "/",
              "value": "End of list"
            }
          ]
        },
        {
          "function": "build_hierarchy",
          "expect": {
            "/": [
              "Production servers",
              "End of list"
            ],
            "servers": [
              "web1",
              "web2",
              "web3"
            ]
          }
        },
        {
          "function": "get_list",
          "expect": [
            "web1",
            "web2",
            "web3"
          ],
          "args": [
            "servers"
          ]
        }
      ],
      "features": [
        "comments"
      ]
    },
    {
      "name": "list_error_missing_key",
      "input": "existing = value",
      "tests": [
        {
          "function": "build_hierarchy",
          "expect": {
            "existing": "value"
          }
        },
        {
          "function": "get_list",
          "expect": null,
          "args": [
            "missing"
          ]
        },
        {
          "function": "parse",
          "expect": [
            {
              "key": "existing",
              "value": "value"
            }
          ]
        }
      ]
    },
