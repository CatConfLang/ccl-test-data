{
  "$schema": "../../schemas/source-format.json",
  "tests": [
    {
      "name": "composition_stability_duplicate_keys",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "a",
              "value": "1"
            },
            {
              "key": "b",
              "value": "2"
            },
            {
              "key": "b",
              "value": "20"
            },
            {
              "key": "c",
              "value": "3"
            }
          ]
        }
      ],
      "inputs": [
        "a = 1\nb = 2\nb = 20\nc = 3"
      ]
    },
    {
      "name": "multiple_values_same_key",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "ports",
              "value": "8000"
            },
            {
              "key": "ports",
              "value": "8001"
            },
            {
              "key": "ports",
              "value": "8002"
            }
          ]
        }
      ],
      "inputs": [
        "ports = 8000\nports = 8001\nports = 8002"
      ]
    },
    {
      "name": "list_with_empty_keys",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "3"
            },
            {
              "key": "",
              "value": "1"
            },
            {
              "key": "",
              "value": "2"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "= 3\n= 1\n= 2"
      ]
    },
    {
      "name": "section_style_syntax",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Section 2 =="
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Section 2 =="
      ]
    },
    {
      "name": "composition_stability_ba",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "b",
              "value": "20"
            },
            {
              "key": "c",
              "value": "3"
            },
            {
              "key": "a",
              "value": "1"
            },
            {
              "key": "b",
              "value": "2"
            }
          ]
        }
      ],
      "inputs": [
        "b = 20\nc = 3\na = 1\nb = 2"
      ]
    },
    {
      "name": "mixed_keys_with_duplicates",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "name",
              "value": "app"
            },
            {
              "key": "ports",
              "value": "8000"
            },
            {
              "key": "name",
              "value": "service"
            },
            {
              "key": "ports",
              "value": "8001"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "name = app\nports = 8000\nname = service\nports = 8001"
      ]
    },
    {
      "name": "array_style_list",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "1",
              "value": ""
            },
            {
              "key": "2",
              "value": ""
            },
            {
              "key": "3",
              "value": ""
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "1 =\n2 =\n3 ="
      ]
    },
    {
      "name": "section_header_double_equals",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Database Config =="
            },
            {
              "key": "host",
              "value": "localhost"
            },
            {
              "key": "port",
              "value": "5432"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Database Config ==\nhost = localhost\nport = 5432"
      ]
    },
    {
      "name": "section_header_triple_equals",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "== Server Settings ==="
            },
            {
              "key": "host",
              "value": "0.0.0.0"
            },
            {
              "key": "ssl",
              "value": "true"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "=== Server Settings ===\nhost = 0.0.0.0\nssl = true"
      ]
    },
    {
      "name": "multiple_sections_with_entries",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Database =="
            },
            {
              "key": "host",
              "value": "localhost"
            },
            {
              "key": "",
              "value": "== Cache ==="
            },
            {
              "key": "redis",
              "value": "enabled"
            },
            {
              "key": "",
              "value": "= Logging =="
            },
            {
              "key": "level",
              "value": "info"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Database ==\nhost = localhost\n\n=== Cache ===\nredis = enabled\n\n== Logging ==\nlevel = info"
      ]
    },
    {
      "name": "section_headers_mixed_with_lists",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Configuration =="
            },
            {
              "key": "",
              "value": "item1"
            },
            {
              "key": "",
              "value": "item2"
            },
            {
              "key": "key",
              "value": "value"
            },
            {
              "key": "",
              "value": "== Next Section ==="
            },
            {
              "key": "other",
              "value": "data"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Configuration ==\n= item1\n= item2\nkey = value\n=== Next Section ===\nother = data"
      ]
    },
    {
      "name": "empty_section_header_only",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Empty Section =="
            }
          ]
        }
      ],
      "inputs": [
        "== Empty Section =="
      ]
    },
    {
      "name": "section_header_at_end",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "key",
              "value": "value"
            },
            {
              "key": "",
              "value": "= Final Section =="
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "key = value\n== Final Section =="
      ]
    },
    {
      "name": "section_headers_no_trailing_equals",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Database Config"
            },
            {
              "key": "host",
              "value": "localhost"
            },
            {
              "key": "",
              "value": "== Server Settings"
            },
            {
              "key": "port",
              "value": "8080"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Database Config\nhost = localhost\n=== Server Settings\nport = 8080"
      ]
    },
    {
      "name": "section_headers_with_colons",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= Database: Production =="
            },
            {
              "key": "host",
              "value": "db.prod.com"
            },
            {
              "key": "",
              "value": "== Cache: Redis Config ==="
            },
            {
              "key": "port",
              "value": "6379"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== Database: Production ==\nhost = db.prod.com\n=== Cache: Redis Config ===\nport = 6379"
      ]
    },
    {
      "name": "spaced_equals_not_section_header",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= spaced equals"
            },
            {
              "key": "",
              "value": "= wide spaces"
            },
            {
              "key": "",
              "value": "= Real Header =="
            },
            {
              "key": "key",
              "value": "value"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "= = spaced equals\n=  = wide spaces\n== Real Header ==\nkey = value"
      ]
    },
    {
      "name": "consecutive_section_headers",
      "tests": [
        {
          "function": "parse",
          "expect": [
            {
              "key": "",
              "value": "= First Section =="
            },
            {
              "key": "",
              "value": "== Nested Section ==="
            },
            {
              "key": "",
              "value": "=== Deep Section ===="
            },
            {
              "key": "key",
              "value": "value"
            }
          ]
        }
      ],
      "features": [
        "empty_keys"
      ],
      "inputs": [
        "== First Section ==\n=== Nested Section ===\n==== Deep Section ====\nkey = value"
      ]
    }
  ]
}
