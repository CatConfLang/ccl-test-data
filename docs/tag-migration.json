{
  "tag_migration": {
    "description": "Mapping from old descriptive tags to new functional tags",
    "migration_map": {
      "basic": ["function:parse"],
      "essential-parsing": ["function:parse"],
      "empty": [],
      "empty-value": ["feature:empty_keys"],
      "empty-key": ["feature:empty_keys"],
      "empty-multiline": ["feature:multiline", "feature:empty_keys"],

      "lists": ["feature:empty_keys"],
      "nested": ["function:build_hierarchy"],
      "multiline": ["feature:multiline"],
      "whitespace": ["feature:whitespace"],
      "unicode": ["feature:unicode"],

      "complex": ["function:build_hierarchy"],
      "mixed": ["function:build_hierarchy", "feature:empty_keys"],
      "deep": ["function:build_hierarchy"],

      "crlf": ["behavior:crlf_preserve_literal"],
      "tabs": ["behavior:tabs_preserve"],
      "spacing": ["behavior:strict_spacing"],

      "round-trip": ["function:pretty_print"],
      "canonical": ["function:pretty_print"],
      "deterministic": ["function:pretty_print"],
      "order": ["function:pretty_print"],

      "algebraic": ["function:compose"],
      "associativity": ["function:compose"],
      "semigroup": ["function:compose"],
      "monoid": ["function:compose"],
      "identity": ["function:compose"],
      "left": ["function:compose"],
      "right": ["function:compose"],

      "proposed-behavior": ["variant:proposed_behavior"],
      "reference-compliant-behavior": ["variant:reference_compliant"]
    }
  },
  
  "tag_categories": {
    "description": "Organized categories of the new tag system",
    "function": {
      "description": "CCL function requirements",
      "tags": [
        "parse", "filter", "compose", "expand_dotted",
        "build_hierarchy", "get_string", "get_int", "get_bool",
        "get_float", "get_list", "pretty_print"
      ]
    },
    "feature": {
      "description": "CCL language features that may be optional",
      "tags": [
        "comments", "experimental_dotted_keys", "empty_keys", "multiline",
        "unicode", "whitespace"
      ]
    },
    "behavior": {
      "description": "Implementation behavioral choices (mutually exclusive within groups)",
      "tags": [
        "crlf_preserve_literal", "crlf_normalize_to_lf", "boolean_lenient", "boolean_strict",
        "tabs_preserve", "tabs_to_spaces", "strict_spacing", "loose_spacing"
      ]
    },
    "variant": {
      "description": "Implementation variant choices for specification evolution",
      "tags": [
        "proposed_behavior", "reference_compliant"
      ]
    }
  },
  
  "automatic_conflicts": {
    "description": "Tags that automatically conflict with each other",
    "conflicts": {
      "behavior:crlf_preserve_literal": ["behavior:crlf_normalize_to_lf"],
      "behavior:boolean_lenient": ["behavior:boolean_strict"],
      "behavior:tabs_preserve": ["behavior:tabs_to_spaces"],
      "behavior:strict_spacing": ["behavior:loose_spacing"],
      "variant:proposed_behavior": ["variant:reference_compliant"]
    }
  },
  
  "usage_examples": {
    "minimal_implementation": {
      "description": "Basic Parse() only implementation",
      "supported_tags": ["function:parse"],
      "skip_tags": ["function:filter", "function:compose", "function:build_hierarchy", "function:get_string", "function:get_int", "function:get_bool", "function:get_float", "function:get_list", "function:pretty_print"]
    },
    "basic_implementation": {
      "description": "Parse + Objects + Typed Access implementation (current mock stage)",
      "supported_tags": ["function:parse", "function:build_hierarchy", "function:get_string", "function:get_int", "function:get_bool", "function:get_float", "function:get_list"],
      "supported_features": ["feature:experimental_dotted_keys", "feature:empty_keys"],
      "behavioral_choices": {
        "line_endings": "behavior:crlf_normalize_to_lf",
        "boolean_parsing": "behavior:boolean_lenient",
        "whitespace": "behavior:loose_spacing"
      }
    },
    "reference_compliant": {
      "description": "Implementation following existing reference behavior",
      "variant_choice": "variant:reference_compliant",
      "conflicts_with": ["variant:proposed_behavior"]
    }
  }
}