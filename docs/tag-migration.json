{
  "tag_migration": {
    "description": "Mapping from old descriptive tags to new functional tags",
    "migration_map": {
      "basic": ["function:parse"],
      "essential-parsing": ["function:parse"], 
      "empty": [],
      "empty-value": ["feature:empty-keys"],
      "empty-key": ["feature:empty-keys"],
      "empty-multiline": ["feature:multiline", "feature:empty-keys"],
      
      "lists": ["feature:empty-keys"],
      "nested": ["function:make-objects"],
      "multiline": ["feature:multiline"],
      "whitespace": ["feature:whitespace"],
      "unicode": ["feature:unicode"],
      
      "complex": ["function:make-objects"],
      "mixed": ["function:make-objects", "feature:empty-keys"],
      "deep": ["function:make-objects"],
      
      "crlf": ["behavior:crlf-preserve-literal"],
      "tabs": ["behavior:tabs-preserve"], 
      "spacing": ["behavior:strict-spacing"],
      
      "round-trip": ["function:pretty-print"],
      "canonical": ["function:pretty-print"],
      "deterministic": ["function:pretty-print"],
      "order": ["function:pretty-print"],
      
      "algebraic": ["function:compose"],
      "associativity": ["function:compose"],
      "semigroup": ["function:compose"],
      "monoid": ["function:compose"],
      "identity": ["function:compose"],
      "left": ["function:compose"],
      "right": ["function:compose"],
      
      "proposed-behavior": ["variant:proposed-behavior"],
      "reference-compliant-behavior": ["variant:reference-compliant"]
    }
  },
  
  "tag_categories": {
    "description": "Organized categories of the new tag system",
    "function": {
      "description": "CCL function requirements (Level 1-5)",
      "tags": [
        "parse", "filter", "compose", "expand-dotted", 
        "make-objects", "get-string", "get-int", "get-bool", 
        "get-float", "get-list", "pretty-print"
      ]
    },
    "feature": {
      "description": "CCL language features that may be optional",
      "tags": [
        "comments", "dotted-keys", "empty-keys", "multiline", 
        "unicode", "whitespace"
      ]
    },
    "behavior": {
      "description": "Implementation behavioral choices (mutually exclusive within groups)",
      "tags": [
        "crlf-preserve-literal", "crlf-normalize-to-lf", "boolean-lenient", "boolean-strict",
        "tabs-preserve", "tabs-to-spaces", "strict-spacing", "loose-spacing"
      ]
    },
    "variant": {
      "description": "Implementation variant choices for specification evolution",
      "tags": [
        "proposed-behavior", "reference-compliant"
      ]
    }
  },
  
  "automatic_conflicts": {
    "description": "Tags that automatically conflict with each other",
    "conflicts": {
      "behavior:crlf-preserve-literal": ["behavior:crlf-normalize-to-lf"],
      "behavior:boolean-lenient": ["behavior:boolean-strict"],
      "behavior:tabs-preserve": ["behavior:tabs-to-spaces"],
      "behavior:strict-spacing": ["behavior:loose-spacing"],
      "variant:proposed-behavior": ["variant:reference-compliant"]
    }
  },
  
  "usage_examples": {
    "minimal_implementation": {
      "description": "Basic Parse() only implementation",
      "supported_tags": ["function:parse"],
      "skip_tags": ["function:filter", "function:compose", "function:make-objects", "function:get-string", "function:get-int", "function:get-bool", "function:get-float", "function:get-list", "function:pretty-print"]
    },
    "basic_implementation": {
      "description": "Parse + Objects + Typed Access implementation (current mock level)",
      "supported_tags": ["function:parse", "function:make-objects", "function:get-string", "function:get-int", "function:get-bool", "function:get-float", "function:get-list"],
      "supported_features": ["feature:dotted-keys", "feature:empty-keys"],
      "behavioral_choices": {
        "line_endings": "behavior:crlf-normalize-to-lf",
        "boolean_parsing": "behavior:boolean-lenient", 
        "whitespace": "behavior:loose-spacing"
      }
    },
    "reference_compliant": {
      "description": "Implementation following existing reference behavior",
      "variant_choice": "variant:reference-compliant",
      "conflicts_with": ["variant:proposed-behavior"]
    }
  }
}