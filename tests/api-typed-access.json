      "name": "parse_basic_integer",
      "input": "port = 8080",
      "validations": {
        "parse": {
          "count": 1,
          "expected": [
            {
              "key": "port",
              "value": "8080"
            }
          ]
        },
        "make_objects": {
          "count": 1,
          "expected": {
            "port": "8080"
          }
        },
        "get_int": {
          "count": 1,
          "cases": [
            {
              "args": [
                "port"
              ],
              "expected": 8080
            }
          ]
        }
      },
      "meta": {
        "tags": [
          "function:get-int",
          "function:make-objects",
          "function:parse"
        ],
        "level": 4,
        "feature": "typed-parsing"
      }
    },
