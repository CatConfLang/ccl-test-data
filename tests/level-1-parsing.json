{
  "suite": "CCL Entry Parsing",
  "version": "1.0",
  "description": "Level 1: Core parsing from text to Entry[] - essential for CCL compliance",
  "tests": [
    {
      "name": "basic_pairs",
      "input": "name = Alice\nage = 42",
      "expected": [
        {"key": "name", "value": "Alice"},
        {"key": "age", "value": "42"}
      ],
      "meta": {"tags": ["basic"], "level": 1}
    },
    {
      "name": "equals_in_values", 
      "input": "msg = k=v pairs live happily here\nmore = a=b=c=d",
      "expected": [
        {"key": "msg", "value": "k=v pairs live happily here"},
        {"key": "more", "value": "a=b=c=d"}
      ],
      "meta": {"tags": ["basic", "equals"], "level": 1}
    },
    {
      "name": "trimming_rules",
      "input": "  spaces around key   =    value with leading spaces removed and trailing tabs kept? \t\t",
      "expected": [
        {"key": "spaces around key", "value": "value with leading spaces removed and trailing tabs kept?"}
      ],
      "meta": {"tags": ["whitespace", "trimming"], "level": 1}
    },
    {
      "name": "multiline_values",
      "input": "description = First\n  Second line\n  Third line\ndone = yes",
      "expected": [
        {"key": "description", "value": "First\n  Second line\n  Third line"},
        {"key": "done", "value": "yes"}
      ],
      "meta": {"tags": ["multiline", "continuation"], "level": 1}
    },
    {
      "name": "blank_lines_in_values",
      "input": "body = Line one\n\n  Line three after a blank line",
      "expected": [
        {"key": "body", "value": "Line one\n\n  Line three after a blank line"}
      ],
      "meta": {"tags": ["multiline", "blank-lines"], "level": 1}
    },
    {
      "name": "no_equals_continuation",
      "input": "key = first line\nsecond line without equals\nthird line",
      "expected": [
        {"key": "key", "value": "first line\nsecond line without equals\nthird line"}
      ],
      "meta": {"tags": ["continuation", "multiline"], "level": 1}
    },
    {
      "name": "key_only_empty_value",
      "input": "emptykey =\nother = value",
      "expected": [
        {"key": "emptykey", "value": ""},
        {"key": "other", "value": "value"}
      ],
      "meta": {"tags": ["empty-value", "basic"], "level": 1}
    },
    {
      "name": "empty_key_with_value",
      "input": "= val",
      "expected": [
        {"key": "", "value": "val"}
      ],
      "meta": {"tags": ["empty-key", "lists"], "level": 1}
    },
    {
      "name": "empty_key_and_value",
      "input": "=",
      "expected": [
        {"key": "", "value": ""}
      ],
      "meta": {"tags": ["empty-key", "empty-value"], "level": 1}
    },
    {
      "name": "unicode_graphemes",
      "input": "emoji = üòÄüòÉüòÑ",
      "expected": [
        {"key": "emoji", "value": "üòÄüòÉüòÑ"}
      ],
      "meta": {"tags": ["unicode", "basic"], "level": 1}
    },
    {
      "name": "unicode_keys",
      "input": "ÈÖçÁΩÆ = config in Chinese\n–∫–ª—é—á = key in Russian",
      "expected": [
        {"key": "ÈÖçÁΩÆ", "value": "config in Chinese"},
        {"key": "–∫–ª—é—á", "value": "key in Russian"}
      ],
      "meta": {"tags": ["unicode", "keys"], "level": 1}
    },
    {
      "name": "crlf_normalization",
      "input": "key1 = value1\r\nkey2 = value2\r\n",
      "expected": [
        {"key": "key1", "value": "value1"},
        {"key": "key2", "value": "value2"}
      ],
      "meta": {"tags": ["line-endings", "normalization"], "level": 1}
    },
    {
      "name": "empty_input",
      "input": "",
      "expected": [],
      "meta": {"tags": ["edge-cases", "empty"], "level": 1}
    },
    {
      "name": "eof_without_newline",
      "input": "last = final value",
      "expected": [
        {"key": "last", "value": "final value"}
      ],
      "meta": {"tags": ["edge-cases", "eof"], "level": 1}
    },
    {
      "name": "tab_preservation_in_values",
      "input": "mixed =    \tvalue with leading spaces and tabs\t",
      "expected": [
        {"key": "mixed", "value": "\tvalue with leading spaces and tabs"}
      ],
      "meta": {"tags": ["whitespace", "tabs", "trimming"], "level": 1}
    },
    {
      "name": "mixed_indentation_continuation",
      "input": "description = First line\n    spaces indented\n\ttab indented\n  \tmixed indent",
      "expected": [
        {"key": "description", "value": "First line\n    spaces indented\n\ttab indented\n  \tmixed indent"}
      ],
      "meta": {"tags": ["multiline", "indentation", "mixed-whitespace"], "level": 1}
    },
    {
      "name": "indented_equals_continuation",
      "input": "key1 = val1\n  inner = some\nkey2 = val2",
      "expected": [
        {"key": "key1", "value": "val1\n  inner = some"},
        {"key": "key2", "value": "val2"}
      ],
      "meta": {"tags": ["continuation", "indentation", "equals"], "level": 1}
    },
    {
      "name": "nested_key_value_pairs",
      "input": "key =\n  field1 = value1\n  field2 = value2",
      "expected": [
        {"key": "key", "value": "\n  field1 = value1\n  field2 = value2"}
      ],
      "meta": {"tags": ["nested", "indentation", "equals", "continuation"], "level": 1}
    },
    {
      "name": "basic_single_no_spaces",
      "input": "key=val",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["basic", "redundant"], "level": 1}
    },
    {
      "name": "basic_with_spaces", 
      "input": "key = val",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["basic", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "indented_key",
      "input": "  key = val",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["whitespace", "indentation", "redundant"], "level": 1}
    },
    {
      "name": "value_trailing_spaces",
      "input": "key = val  ",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["whitespace", "trimming", "redundant"], "level": 1}
    },
    {
      "name": "key_value_surrounded_spaces",
      "input": "  key  =  val  ",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["whitespace", "trimming", "redundant"], "level": 1}
    },
    {
      "name": "surrounded_by_newlines",
      "input": "\nkey = val\n",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["line-endings", "redundant"], "level": 1}
    },
    {
      "name": "key_empty_value",
      "input": "key =",
      "expected": [
        {"key": "key", "value": ""}
      ],
      "meta": {"tags": ["empty-value", "redundant"], "level": 1}
    },
    {
      "name": "empty_value_with_newline",
      "input": "key =\n",
      "expected": [
        {"key": "key", "value": ""}
      ],
      "meta": {"tags": ["empty-value", "line-endings", "redundant"], "level": 1}
    },
    {
      "name": "empty_value_with_spaces",
      "input": "key =  ",
      "expected": [
        {"key": "key", "value": ""}
      ],
      "meta": {"tags": ["empty-value", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "empty_key_indented",
      "input": "  = val",
      "expected": [
        {"key": "", "value": "val"}
      ],
      "meta": {"tags": ["empty-key", "lists", "indentation", "redundant"], "level": 1}
    },
    {
      "name": "empty_key_with_newline",
      "input": "\n  = val",
      "expected": [
        {"key": "", "value": "val"}
      ],
      "meta": {"tags": ["empty-key", "lists", "line-endings", "redundant"], "level": 1}
    },
    {
      "name": "empty_key_value_with_spaces",
      "input": "  =  ",
      "expected": [
        {"key": "", "value": ""}
      ],
      "meta": {"tags": ["empty-key", "empty-value", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "equals_in_value_no_spaces",
      "input": "a=b=c",
      "expected": [
        {"key": "a", "value": "b=c"}
      ],
      "meta": {"tags": ["equals", "basic", "redundant"], "level": 1}
    },
    {
      "name": "equals_in_value_with_spaces",
      "input": "a = b = c",
      "expected": [
        {"key": "a", "value": "b = c"}
      ],
      "meta": {"tags": ["equals", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "multiple_key_value_pairs",
      "input": "key1 = val1\nkey2 = val2",
      "expected": [
        {"key": "key1", "value": "val1"},
        {"key": "key2", "value": "val2"}
      ],
      "meta": {"tags": ["basic", "multiple", "redundant"], "level": 1}
    },
    {
      "name": "key_with_tabs",
      "input": "\tkey\t=\tvalue",
      "expected": [
        {"key": "key", "value": "\tvalue"}
      ],
      "meta": {"tags": ["whitespace", "tabs", "redundant"], "level": 1}
    },
    {
      "name": "whitespace_only_value",
      "input": "onlyspaces =     ",
      "expected": [
        {"key": "onlyspaces", "value": ""}
      ],
      "meta": {"tags": ["whitespace", "empty-value", "redundant"], "level": 1}
    },
    {
      "name": "spaces_vs_tabs_continuation",
      "input": "text = First\n    four spaces\n\ttab preserved",
      "expected": [
        {"key": "text", "value": "First\n    four spaces\n\ttab preserved"}
      ],
      "meta": {"tags": ["continuation", "whitespace", "tabs", "redundant"], "level": 1}
    },
    {
      "name": "multiple_empty_equality",
      "input": " =  = ",
      "expected": [
        {"key": "", "value": "="}
      ],
      "meta": {"tags": ["empty-key", "multiple-equals", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "key_with_newline_before_equals",
      "input": "key \n= val\n",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["whitespace", "newlines", "edge-case", "redundant"], "level": 1}
    },
    {
      "name": "complex_multi_newline_whitespace",
      "input": "  \n key  \n=  val  \n",
      "expected": [
        {"key": "key", "value": "val"}
      ],
      "meta": {"tags": ["whitespace", "newlines", "edge-case", "redundant"], "level": 1}
    },
    {
      "name": "empty_value_with_trailing_spaces_newline",
      "input": "key =  \n",
      "expected": [
        {"key": "key", "value": ""}
      ],
      "meta": {"tags": ["empty-value", "whitespace", "newlines", "redundant"], "level": 1}
    },
    {
      "name": "empty_key_value_with_surrounding_newlines",
      "input": "\n  =  \n",
      "expected": [
        {"key": "", "value": ""}
      ],
      "meta": {"tags": ["empty-key", "empty-value", "newlines", "whitespace", "redundant"], "level": 1}
    },
    {
      "name": "quotes_treated_as_literal_unquoted",
      "input": "host = localhost",
      "expected": [
        {"key": "host", "value": "localhost"}
      ],
      "meta": {"tags": ["quotes", "literal", "redundant"], "level": 1}
    },
    {
      "name": "quotes_treated_as_literal_quoted",
      "input": "host = \"localhost\"",
      "expected": [
        {"key": "host", "value": "\"localhost\""}
      ],
      "meta": {"tags": ["quotes", "literal", "redundant"], "level": 1}
    },
    {
      "name": "nested_single_line",
      "input": "key =\n  val",
      "expected": [
        {"key": "key", "value": "\n  val"}
      ],
      "meta": {"tags": ["nested", "indentation", "redundant"], "level": 1}
    },
    {
      "name": "nested_multi_line",
      "input": "key =\n  line1\n  line2",
      "expected": [
        {"key": "key", "value": "\n  line1\n  line2"}
      ],
      "meta": {"tags": ["nested", "indentation", "multiline", "redundant"], "level": 1}
    },
    {
      "name": "nested_with_blank_line",
      "input": "key =\n  line1\n\n  line2",
      "expected": [
        {"key": "key", "value": "\n  line1\n\n  line2"}
      ],
      "meta": {"tags": ["nested", "indentation", "multiline", "blank-lines", "redundant"], "level": 1}
    },
    {
      "name": "deep_nested_structure",
      "input": "key =\n  field1 = value1\n  field2 =\n    subfield = x\n    another = y",
      "expected": [
        {"key": "key", "value": "\n  field1 = value1\n  field2 =\n    subfield = x\n    another = y"}
      ],
      "meta": {"tags": ["nested", "indentation", "deep", "continuation", "redundant"], "level": 1}
    },
    {
      "name": "realistic_stress_test",
      "input": "name = Dmitrii Kovanikov\nlogin = chshersh\nlanguage = OCaml\ndate = 2024-05-25",
      "expected": [
        {"key": "name", "value": "Dmitrii Kovanikov"},
        {"key": "login", "value": "chshersh"},
        {"key": "language", "value": "OCaml"},
        {"key": "date", "value": "2024-05-25"}
      ],
      "meta": {"tags": ["realistic", "multiple-pairs", "stress-test", "redundant"], "level": 1}
    }
  ]
}