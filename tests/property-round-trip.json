{
  "$schema": "./schema.json",
  "suite": "CCL Pretty Printer (Validation Format)",
  "version": "2.0", 
  "description": "Pretty printing and round-trip testing - ensures canonical formatting and parse/pretty-print/parse identity",
  "tests": [
    {
      "name": "round_trip_basic",
      "input": "key = value\nnested =\n  sub = val",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "basic"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_whitespace_normalization",
      "input": "  key  =  value  \n  nested  = \n    sub  =  val  ",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "whitespace"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_empty_keys_lists",
      "input": "= item1\n= item2\nregular = value",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "lists"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_nested_structures",
      "input": "config =\n  host = localhost\n  port = 8080\n  db =\n    name = mydb\n    user = admin",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "nested"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_multiline_values",
      "input": "script =\n  #!/bin/bash\n  echo hello\n  exit 0",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "multiline"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_mixed_content",
      "input": "name = Alice\n= first item\nconfig =\n  port = 3000\n= second item\nfinal = value",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "mixed"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "canonical_format_empty_values",
      "input": "empty_key =",
      "validations": {
        "canonical_format": {
          "expected": "empty_key ="
        }
      },
      "meta": {"tags": ["canonical", "empty"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "canonical_format_tab_preservation",
      "input": "value_with_tabs = text\t\twith\ttabs\t",
      "validations": {
        "canonical_format": {
          "expected": "value_with_tabs = text\t\twith\ttabs"
        }
      },
      "meta": {"tags": ["canonical", "tabs"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "canonical_format_unicode",
      "input": "unicode = ä½ å¥½ä¸–ç•Œ\nemo = ðŸŒŸâœ¨",
      "validations": {
        "canonical_format": {
          "expected": "unicode = ä½ å¥½ä¸–ç•Œ\nemo = ðŸŒŸâœ¨"
        }
      },
      "meta": {"tags": ["canonical", "unicode"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_complex_nesting",
      "input": "app =\n  = item1\n  config =\n    = nested_item\n    db =\n      host = localhost\n      = db_item\n  = item2",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "complex"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "canonical_format_line_endings",
      "input": "key1 = value1\r\nkey2 = value2\r\n",
      "validations": {
        "canonical_format": {
          "expected": "key1 = value1\nkey2 = value2"
        }
      },
      "meta": {"tags": ["canonical", "crlf"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_deeply_nested",
      "input": "level1 =\n  level2 =\n    level3 =\n      level4 =\n        deep = value\n        = deep_item",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "deep"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "canonical_format_consistent_spacing",
      "input": "key1=value1\nkey2  =  value2\nkey3\t=\tvalue3",
      "validations": {
        "canonical_format": {
          "expected": "key1 = value1\nkey2 = value2\nkey3 = \tvalue3"
        }
      },
      "meta": {"tags": ["canonical", "spacing"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "round_trip_empty_multiline",
      "input": "empty_section =\n\nother = value",
      "validations": {
        "round_trip": {
          "property": "identity"
        }
      },
      "meta": {"tags": ["round-trip", "empty-multiline"], "level": 4, "feature": "pretty-printing"}
    },
    {
      "name": "deterministic_output",
      "input": "z = last\na = first\nm = middle",
      "validations": {
        "canonical_format": {
          "expected": "z = last\na = first\nm = middle"
        }
      },
      "meta": {"tags": ["deterministic", "order"], "level": 4, "feature": "pretty-printing"}
    }
  ]
}