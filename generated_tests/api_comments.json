  "tests": [
    {
      "name": "comment_extension_parse",
      "input": "/= This is an environment section\nport = 8080\nserve = index.html\n/= Database section\nmode = in-memory\nconnections = 16",
      "validation": "parse",
      "expected": [
        {
          "key": "/",
          "value": "This is an environment section"
